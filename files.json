{
	"packages": [
		"coreutils",
		"dnsmasq",
		"hostapd",
		"conntrack",
		"nodejs"
	],
	"comment": "Don't forget to add a trailing slash after directory names!",
	"folders": {
		"/usr/bin/": [
			"issueToken.js",
			"rmtrack",
			"server.js"
		],
		"/etc/hotspot/": [
			"profiles.json",
			"users.json"
		],
		"/etc/hotspot/lib/": [
			"tokens.js",
			"profiles.js"
		],
		"/etc/hotspot/html/": [
			"blocked.htm",
			"welcome.htm",
			"unlocked.htm",
			"wrongCode.htm",
			"error.htm"
		]
	}
}