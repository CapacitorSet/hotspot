{
	"packages": [
		"coreutils",
		"dnsmasq",
		"hostapd",
		"conntrack",
		"iw",
		"wireless-tools"
	],
	"comments": [
		"Don't forget the trailing slash after directory names!",
		"Libraries are to be stored in 'libs', not in 'lib'!"
	],
	"folders": {
		"/usr/bin/": [
			"server.js",
			"issueToken.js",
			"rmtrack"
		],
		"/etc/hotspot/": [
			"profiles.json",
			"users.json"
		],
		"/etc/hotspot/lib/": [
			"profiles.js",
			"tokens.js"
		],
		"/etc/hotspot/html/": [
			"html/blocked.htm",
			"html/error.htm",
			"html/unlocked.htm",
			"html/welcome.htm",
			"html/wrongCode.htm",
			"html/welcomeFullAccess.htm"
		],
		"/etc/hotspot/tokens/": []
	}
}